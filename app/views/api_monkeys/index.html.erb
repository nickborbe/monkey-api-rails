<p id="notice"><%= notice %></p>

<h1>Api Monkeys</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_monkeys.each do |api_monkey| %>
      <tr>
        <td><%= link_to 'Show', api_monkey %></td>
        <td><%= link_to 'Edit', edit_api_monkey_path(api_monkey) %></td>
        <td><%= link_to 'Destroy', api_monkey, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Monkey', new_api_monkey_path %>
